<aura:component controller="CSVGenFromLstViewController">
    <aura:attribute name="ObjType" type="String" access="global"/>
    <aura:attribute name="redirectURL" type="String" access="global"/>
    <aura:attribute name="seletedListView" type="String" access="global"/>
    <aura:attribute name="headerList" type="String" access="global"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>    
    <div class="demo-only demo-only--sizing slds-grid slds-wrap">
        <div class="slds-size_1-of-2">
            <div class="slds-box slds-box_x-small slds-text-align_center slds-m-around_x-small"><ui:inputSelect aura:id="accIndustry" class="slds-select"  change="{!c.onPicklistChange}"/></div>
        </div>
        <div class="slds-size_1-of-2">
            <div class="slds-box slds-box_x-small slds-text-align_center slds-m-around_x-small"><ui:button aura:id="button" buttonTitle="Click to see what you put into the field" class="button" label="Generate CSV" press="{!c.GenerateCSV}"/></div>
        </div>
    </div>
</aura:component>