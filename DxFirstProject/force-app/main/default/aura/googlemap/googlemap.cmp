<aura:component implements="force:appHostable" >
    
 <aura:handler name="init" value="{!this}"
    action="{!c.handleComponentEvent}"/> 
	<aura:attribute name="vfHost" type="String" default="" />
    
    <aura:attribute name="mapOptions" type="Object" default='{"zoom": 4}' />
        
    <aura:attribute name="mapdata" type="Object[]" />
    <aura:attribute name="customLabelValue" type="string"/>
    
    <aura:attribute name="loadMap" type="Boolean" default="false" />
    
    
    <aura:attribute name="lcHost" type="String" />
    
    
   <!-- VF page hosted inside a lightning component(map element will be created inside this vf page) />-->
   
	<iframe aura:id="vfFrame" src="{! '/apex/googlemapvf?lcHost=' + v.lcHost}" 
            width="100%" height="73%" />
    
</aura:component>