<apex:page standardController="Opportunity" tabStyle="Opportunity" recordSetVar="AllOpps">
	<apex:repeat value="{!AllOpps}" var="row" rendered="false">
    
        	{!row.Id} {!row.OwnerId}
    
    </apex:repeat>
    <flow:interview name="mass_bulk_update" finishLocation="{!URLFOR($Action.Opportunity.Tab, $ObjectType.Opportunity)}">
        <apex:param name="varSelectedOpps" value="{!Selected}"/>
    </flow:interview>
                 
</apex:page>